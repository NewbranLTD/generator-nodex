{
  "name": "generator-nodex",
  "version": "3.1.10",
  "description":
    "使用语言选项创建 node.js 模块 / Create a Node.js module with language option. Pass `yo nodex --lang=cn | en` ",
  "homepage": "https://github.com/NewBrandLTD/generator-nodex",
  "author": "NEWBRAN LTD",
  "files": ["index.js", "generators", "lib"],
  "main": "index.js",
  "keywords": ["yeoman-generator", "scaffold", "node", "module", "cli"],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lint-staged": "^5.0.0",
    "nsp": "^3.1.0",
    "prettier": "^1.8.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "repository": "git@github.com:NewbranLTD/generator-nodex.git",
  "scripts": {
    "pretest": "eslint . --fix",
    "test": "jest --runInBand --forceExit",
    "_prepublishOnly": "nsp check && npm test",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "command-exists": "^1.2.2",
    "fs-extra": "^4.0.2",
    "generator-jest": "^1.4.2",
    "generator-license": "^5.2.0",
    "generator-travis": "^1.5.3",
    "git-remote-origin-url": "^2.0.0",
    "github-username": "^4.1.0",
    "glob": "^7.1.2",
    "inquirer-npm-name": "^2.0.0",
    "lodash": "^4.17.4",
    "npm-check-updates": "^2.13.0",
    "parse-author": "^2.0.0",
    "username": "^3.0.0",
    "when": "^3.7.8",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["/fixtures/"]
  },
  "engines": {
    "node": ">=7.00"
  }
}
